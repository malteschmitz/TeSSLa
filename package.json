{
  "name": "tessla",
  "main": "./lib/tessla",
  "version": "1.3.0",
  "description": "This packages provides some IDE-like functions for C-Code and TeSSLa specifications",
  "keywords": [
    "tessla",
    "tessla-ide",
    "ide"
  ],
  "activationCommands": {
    "atom-workspace": "tessla:toggle"
  },
  "repository": "https://github.com/malteschmitz/tessla-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.3.1",
    "copy-dir": "^0.3.0",
    "download-file": "^0.1.5",
    "fs-extra": "^3.0.0",
    "install": "^0.8.9",
    "jquery": "^3.1.1",
    "npm": "^4.5.0",
    "os": "^0.1.1",
    "scan-folder": "^1.0.0",
    "unzip": "^0.1.11"
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "package-deps": [
    "tool-bar",
    "linter",
    "linter-gcc"
  ]
}
